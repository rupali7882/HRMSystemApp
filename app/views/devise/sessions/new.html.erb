<div class="map">
</div>
<section id="contact-page">
   
       
        <div class="row contact-wrap"> 
            <div class="col-sm-8 col-sm-offset-2 loginf">
             <div class="center">        
                  <h2>Log-In</h2>
                  <p class="lead">Company Management System</p>
              </div> 
                <%= form_for(resource, as: resource_name, url: session_path(resource_name), :class=>"contactForm") do |f| %>

                    <div class="form-group">
                      <%= f.email_field :email, autofocus: true, :class=>"form-control", :placeholder=>"Your Email" %>
                    </div>
                    <div class="form-group">
                     <%= f.password_field :password,  autocomplete: "off", :class=>"form-control", :placeholder=>"Your Password" %>
                    </div>
                    <div class="text-center">
                        <%= f.submit "Log in" ,:class=>"btn btn-primary btn-lg" %>
                    </div>
                <% end %>     
            </div>
        </div><!--/.row-->
    </div><!--/.container-->
</section><!--/#contact-page-->

<%#= render "devise/shared/links" %>
